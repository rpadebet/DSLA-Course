   "source": [
    "mtcars$mpg_norm <- (mtcars$mpg - mean(mtcars$mpg))/sd(mtcars$mpg)\n",
    "mtcars$wt_norm <- (mtcars$wt - mean(mtcars$wt))/sd(mtcars$wt)\n",
    "\n",
    "mdl3<-lm(mpg_norm ~ wt_norm,data = mtcars)\n",
    "summary(mdl3)"
